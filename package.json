{
	"name": "svelte-kit",
	"version": "0.0.1",
	"scripts": {
		"dev": "concurrently vite dev & npm run cms",
		"cms": "cd cms && npm run develop",
		"cms-start": "cd cms && npm run start",
		"cms-build": "cd cms && NODE_ENV=production npm run build",
		"build": "vite build && npm run cms-build",
		"preview": "concurrently vite prewview & npm run cms-start",
		"check": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json --watch",
		"lint": "eslint .",
		"start": "concurrently vite prewview & npm run cms-start"
	},
	"devDependencies": {
		"@hi_digital/easy-flex": "^1.6.4",
		"@sveltejs/adapter-auto": "next",
		"@sveltejs/adapter-node": "^1.0.0-next.91",
		"@sveltejs/kit": "next",
		"@types/cookie": "^0.5.1",
		"eslint": "^8.16.0",
		"eslint-plugin-svelte3": "^4.0.0",
		"sass": "^1.54.6",
		"svelte": "^3.46.0",
		"svelte-check": "^2.7.1",
		"svelte-preprocess": "^4.10.7",
		"typescript": "^4.7.4",
		"vite": "^3.1.0-beta.1"
	},
	"type": "module",
	"dependencies": {
		"@fontsource/fira-mono": "^4.5.0",
		"concurrently": "^7.3.0",
		"cookie": "^0.4.1",
		"lazysizes": "^5.3.2",
		"normalize.css": "^8.0.1"
	}
}
